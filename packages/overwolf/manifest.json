{
  "manifest_version": 1,
  "type": "WebApp",
  "meta": {
    "name": "Hogwarts.gg-DEV",
    "author": "Leon Machens",
    "version": "0.3.1",
    "minimum-overwolf-version": "0.218.0",
    "description": "Hogwarts Legacy Map and Tools",
    "dock_button_title": "Hogwarts.gg",
    "icon": "icons/iconMouseOver.png",
    "icon_gray": "icons/iconMouseNormal.png",
    "launcher_icon": "icons/launcherIcon.ico"
  },
  "permissions": ["Hotkeys", "GameInfo", "FileSystem", "DesktopStreaming"],
  "data": {
    "start_window": "controller",
    "windows": {
      "controller": {
        "file": "controller.html",
        "debug_url": "http://localhost:3031/controller.html",
        "is_background_page": true
      },
      "desktop": {
        "file": "index.html",
        "debug_url": "http://localhost:3031/index.html",
        "resizable": true,
        "override_on_update": true,
        "desktop_only": true,
        "transparent": true,
        "native_window": true,
        "size": {
          "width": 1200,
          "height": 800
        },
        "min_size": {
          "width": 900,
          "height": 600
        },
        "allow_local_file_access": true,
        "popup_blocker": true,
        "mute": true,
        "block_top_window_navigation": true
      },
      "overlay": {
        "file": "index.html",
        "debug_url": "http://localhost:3031/index.html",
        "resizable": true,
        "override_on_update": true,
        "transparent": true,
        "in_game_only": true,
        "focus_game_takeover": "ReleaseOnLostFocus",
        "focus_game_takeover_release_hotkey": "toggle_app",
        "size": {
          "width": 1200,
          "height": 800
        },
        "min_size": {
          "width": 900,
          "height": 600
        },
        "allow_local_file_access": true,
        "popup_blocker": true,
        "mute": true,
        "block_top_window_navigation": true
      },
      "authDiscord": {
        "file": "auth-discord.html",
        "debug_url": "http://localhost:3031/auth-discord.html",
        "transparent": false,
        "resizable": true,
        "show_in_taskbar": true,
        "size": {
          "width": 640,
          "height": 700
        }
      },
      "authGitHub": {
        "file": "auth-github.html",
        "debug_url": "http://localhost:3031/auth-github.html",
        "transparent": false,
        "resizable": true,
        "show_in_taskbar": true,
        "size": {
          "width": 640,
          "height": 700
        }
      }
    },
    "game_targeting": {
      "type": "dedicated",
      "game_ids": [22600]
    },
    "game_events": [22600],
    "launch_events": [
      {
        "event": "GameLaunch",
        "event_data": {
          "game_ids": [22600]
        },
        "start_minimized": false
      }
    ],
    "hotkeys": {
      "toggle_app": {
        "title": "Show/Hide App",
        "action-type": "custom",
        "default": "F6"
      }
    },
    "extra-objects": {
      "hlhook": {
        "file": "plugins/HLHookNET4.dll",
        "class": "HogwartsLegacy.Hook"
      }
    },
    "protocol_override_domains": {
      "googlesyndication": "http"
    },
    "externally_connectable": {
      "matches": ["http://localhost:3031", "https://www.hogwarts.gg"]
    },
    "force_browser": "user",
    "developer": {
      "enable_auto_refresh": true,
      "reload_delay": 1000,
      "filter": "*.*"
    }
  }
}
